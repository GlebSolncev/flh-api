{
    "name": "glebsolncev/flh-api",
    "description": "Integration with freelancehunt API, get projects with filter by types",
    "type": "project",
    "require": {
        "guzzlehttp/guzzle": "7.5.x-dev",
        "illuminate/database": "10.x-dev",
        "symfony/http-foundation": "6.3.x-dev",
        "nyholm/psr7": "^1.5",
        "symfony/psr-http-message-bridge": "^2.1@dev",
        "robmorgan/phinx": "1.x-dev",
        "vlucas/phpdotenv": "^5.5@dev",
        "illuminate/pagination": "10.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "10.1.x-dev",
        "symfony/var-dumper": "6.3.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/app/",
            "System\\": "src/system/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "gleb.solncev",
            "email": "hlieb.solntsev@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
